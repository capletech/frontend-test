{
  "name": "oxygen-frontend",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "husky install && ngcc",
    "ng": "ng",
    "start": "ng serve --configuration local",
    "start:frontend-only": "ng serve --configuration frontend-only",
    "cypress": "concurrently \"ng serve\" \"cypress open --browser chrome\"",
    "build": "ng build",
    "build:dev": "ng build --configuration dev",
    "build:demo": "ng build --configuration demo",
    "build:prod": "ng build --configuration production",
    "test:cov": "ng test",
    "test:e2e": "ng e2e",
    "test:integration": "start-server-and-test 4200 'cypress run'",
    "lint": "eslint --ext .js,.ts src && tsc --project tsconfig.app.json",
    "lint:fix": "eslint --fix --ext .js,.ts src && tsc --project tsconfig.app.json",
    "format": "npm run lint:fix && prettier --write \"src/*/.(j|t)s\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/flex-layout": "14.0.0-beta.41",
    "@angular/forms": "^14.3.0",
    "@angular/material": "14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@angular/service-worker": "^14.3.0",
    "@azure/msal-angular": "^2.5.9",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@ngxs/storage-plugin": "3.7.5",
    "@ngxs/store": "3.7.5",
    "angular-auth-oidc-client": "^14.1.5",
    "apexcharts": "^3.45.1",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^2.9.4",
    "dompurify": "^3.2.6",
    "element-scroll-polyfill": "^1.0.1",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.0.0-rc.7",
    "iso-3166": "^4.3.0",
    "jwt-decode": "^3.1.2",
    "marked": "^15.0.12",
    "ng2-charts": "^2.4.3",
    "ng2-pdf-viewer": "^9.1.5",
    "ngx-ellipsis": "^4.1.2",
    "rxjs": "^6.6.7",
    "smoothscroll-polyfill": "^0.4.4",
    "svg-country-flags": "^1.2.10",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular-eslint/eslint-plugin": "^1.2.0",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@ngxs/devtools-plugin": "^3.7.5",
    "@types/canvas-confetti": "^1.9.0",
    "@types/file-saver": "^2.0.3",
    "@types/hammerjs": "^2.0.45",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^5.3.0",
    "cypress": "^6.3.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^5.1.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.12.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.6.3"
  },
  "optionalDependencies": {
    "esbuild-android-arm64": "^0.15.18"
  }
}
